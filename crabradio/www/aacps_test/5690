		{ multimediaRotator: "//extend.schoolwires.com/creative/scripts/creative/rotate/cs.rs.multimedia.rotator.v2.min.js" },
        { globalIcons: "//extend.schoolwires.com/creative/scripts/creative/tools/creative-icons-v3/creativeIcons.v3.min.js" },
        { modEvents: "//extend.schoolwires.com/creative/scripts/joel/mod-events/joel.mod-events.min.js" },
        { csAppAccordion: "//extend.schoolwires.com/creative/scripts/creative/responsive/creative-app-accordion/creative.app.accordion.min.js" },
        { accessibleNavApp: "/cms/lib/MD02215556/Centricity/Template/19/scripts/creative.accessible.navigation.app.min.js" },
        { tabbedApps: "//extend.schoolwires.com/creative/scripts/creative/responsive/cs.rs.tabbed.blocks.min.js" }
	]);
	
	$(window).load(function(){ calendarListView(); searchText(); $("#social-media-outer").slideUp(1).delay(0).slideDown('slow'); });
	
	$(function(){
		$(window).resize(function(){ jsMediaQueries(); calendarListView(); });

        buildSchoolMenu();
        csAppAccordion();
        pageListAccordion();
        useChannelForSection();
        globalIcons();
        multimediaRotator();
        tabbedApps();
        jsMediaQueries();
        dynamicContent();
        modEvents();
    });	
	
	function jsMediaQueries() {
		switch(csGetBreakPoint()) {
			case "desktop": 
				$(".hp-slideshow.right").insertAfter(".hp-slideshow.left");
                $("#gb-mystart-right").insertAfter("#gb-mystart-left");
                $("#hp-slideshow .rs-gallery-desc").insertAfter("#hp-slideshow .rs-gallery-images");
                tabAccordionReset();
			break;
			case "768":
				$(".hp-slideshow.right").prependTo("#hp-content-top");
                $("#gb-mystart-right").insertAfter("#gb-mystart-left");
                $("#hp-slideshow .rs-gallery-desc").insertAfter("#hp-slideshow .rs-gallery-images");
                tabAccordionReset();
			break;
			case "640":
            	$(".hp-slideshow.right").prependTo("#hp-content-top");
				$("#gb-mystart-right").insertAfter("#gb-mystart-left");
                $("#hp-slideshow .rs-gallery-desc").insertAfter("#hp-slideshow .rs-gallery-images");
                tabAccordionReset();
			break;
			case "480":
            	$(".hp-slideshow.right").prependTo("#hp-content-top");
				$("#gb-mystart-right").insertAfter("#gb-mystart-outer");
                $("#hp-slideshow .rs-gallery-desc").insertAfter("#hp-slideshow .rs-gallery-container");
                tabAccordion();
			break;
			case "320":
            	$(".hp-slideshow.right").prependTo("#hp-content-top");
				$("#gb-mystart-right").insertAfter("#gb-mystart-outer");
                $("#hp-slideshow .rs-gallery-desc").insertAfter("#hp-slideshow .rs-gallery-container");
                tabAccordion();
			break;
		}
	}
	
	function multimediaRotator(){
    	head.ready("multimediaRotator", function(){
            if(!$("#sw-content-container10.region.ui-hp .rs-gallery-container").length){
                // MULTIMEDIA ROTATOR
                $("#sw-content-container10.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator").csRsMultimediaRotator({
                    		"imageWidth"              : 658,
                            "imageHeight"             : 394,
                            "efficientLoad"           : "yes",
                            "appHeader"               : "no",
                            "titles"                  : "yes",
                            "captions"                : "yes",
                            "galleryOverlay"          : "no",
                            "linkedElement"           : ["image", "title"], // ["image", "title", "overlay", "all"]
                            "playPause"               : "no",
                            "mainControls"            : "yes",
                            "contMoreLink"            : "no",
                            "contMoreLinkText"        : "Read More",
                            "descMoreLink"            : "no",
                            "descMoreLinkText"        : "Read More",
                            "bullets"                 : "no",
                            "bulletControls"          : "no",
                            "thumbnails"              : "no",
                            "thumbnailWidth"          : 122,
                            "thumbnailHeight"         : 53,
                            "thumbnailSlideDirection" : "horizontal", // horizontal or vertical
                            "hoverPause"              : "yes",
                            "autoRotate"              : "yes",
                            "fullScreenRotator"       : "no", // image dimensions work well at 1500x915
                            "fullScreenBreakpoints"   : ["960", "768", "640", "480", "320"], // OPTIONS ARE ["960", "768", "640", "480", "320"]
                            "fullScreenVideo"         : "no",
                            "transitionType"          : "fade", // fade or slide
                            "transitionSpeed"         : 3,
                            "transitionDelay"         : 8,
                            "onImageLoad"             : function() { },
                            "allImagesLoaded"         : function() { },
                            "onThumbnailLoad"         : function() { },
                            "allThumbnailsLoaded"     : function() { },
                            "onTransitionStart"       : function() { },
                            "onTransitionEnd"         : function() { },
                            "watchVidClick"           : function() { },
                            "closeVidClick"           : function() { },
                            "allLoaded"               : function() { } 
                });
        	}
     	});
	}
    
     // GLOBAL ICONS
    function globalIcons() {
      head.ready("globalIcons", function() {
          $("#gb-global-icons").creativeIcons({
                    "iconNum"       : "8", // NUMBER OF ICONS
                    "icons"         : [
                            ["Site Utility/8.png", "School Calendar", "/Page/2", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                            ["Student Program/43.png", "School Meals", "http://www.aacps.org/nutrition", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                            ["People/29.png", "Student/Parent Portal", "/Page/1325", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    		["/cms/lib011/MD02215556/Centricity/template/globalassets/images/icons/global-icon-paper.png", "Vendors and Contractors", "/Page/1391", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    		["Student Program/8.png", "School Security", "/Page/1039", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                            ["/cms/lib/MD02215556/Centricity/Template/GlobalAssets/icons/57.png", "Safety Hotline", "/Page/8178", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                            ["Classroom/22.png", "News Releases", "http://www.aacpsschools.org/pressr", "_blank"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                            ["Classroom/4.png", "School Year   2019-2020", "/Page/2504", "_self"]  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ],
                    "siteID"        : "4",
                    "siteAlias"     : "aacps",
                    "calendarLink"  : "/Page/2",
                    "contactEmail"  : "info@aacps.org",
                    "allLoaded"     : function(){ 
                    	$('#gb-global-icons .global-image').attr('alt', '');
                        $('#gb-global-icons .global-image').attr('aria-hidden', 'true');
                        $(".global-icon .text").text(function () {
                            return $(this).text().replace("and", "&"); 
                        });
                    }
            });
        });
    }  
    
	function dynamicContent() {
		// ********** MYSTART ********** //
        
        //FIND TODAY'S DATE
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth()+1; //January is 0!
        var yyyy = today.getFullYear();
        if(dd<10){
            dd='0'+dd;
        } 
        if(mm<10){
            mm='0'+mm;
        } 
        var today = mm+'/'+dd+'/'+yyyy;
        
        //INSERT 'SCHOOLS TODAY' AND TODAY'S DATE HEADER WHEN ANNOUNCEMENT IS PRESENT
        $("<div class='mystart-announcement-header'><span class='line-one'>SCHOOLS</span><span class='line-two'>TODAY</span><span class='date'>"+today+"</span>").insertBefore("#gb-mystart-left .ui-article-description");
		
        //IF THERE'S A MYSTART ANNOUNCEMENT APP ON HOMEPAGE, LOAD IT INTO THE SUBPAGE MYSTART
        $(".sp #gb-mystart-left" ).load( "/ #sw-content-container1", function() {
          $("<div class='mystart-announcement-header'><span class='line-one'>SCHOOLS</span><span class='line-two'>TODAY</span><span class='date'>"+today+"</span>").insertBefore(".sp #gb-mystart-left .ui-article-description");
        });
        	
        //REMOVE THE ORIGINAL SITE MANAGER BUTTON
        $("#sw-mystart-right > .sw-mystart-nav.sw-mystart-button.manage").remove();
        
        $(document).off("click", ".sw-mystart-dropdown");
		$(document).on("click", ".sw-mystart-dropdown", function(e) {
			$(".sw-dropdown", this).slideDown(300, "swing");
			$(this).addClass("open");
		});
		$(".sw-mystart-dropdown").hover(function(e) {
			e.preventDefault();
		}, function() {
			$(".sw-dropdown",this).slideUp(300, "swing").attr('aria-hidden', 'true');
			$(this).removeClass("open");
		});
        $(document).off("focus", ".sw-mystart-dropdown");
		$(document).on("focus", ".sw-mystart-dropdown", function(e) {
			$(".sw-dropdown", this).slideDown(300, "swing").attr('aria-hidden', 'false');
			$(this).addClass("open");
		});
        $(".sw-mystart-dropdown").focusout(function() {
          $(this).removeClass("open");
        });
        $(document).off("click", "#sw-mystart-account");
		$(document).on("click", "#sw-mystart-account", function(e) {
			$("#sw-myaccount-list", this).slideDown(300, "swing");
			$(this).addClass("open");
		});
		$(" #sw-mystart-account").hover(function(e) {
			e.preventDefault();
		}, function() {
			$("#sw-myaccount-list",this).slideUp(300, "swing").attr('aria-hidden', 'true');
			$(this).removeClass("open");
		});
        $(document).off("focus", "#sw-mystart-account");
		$(document).on("focus", "#sw-mystart-account", function(e) {
			$("#sw-myaccount-list", this).slideDown(300, "swing").attr('aria-hidden', 'false');
			$(this).addClass("open");
		});
        
        $('<label for="sw-search-input" class="visuallyhidden">Search: </label>').insertBefore('#sw-search-input');
        
        // MOVES MYSTART SEARCH
            $("#sw-mystart-search").detach().appendTo('#gb-tagline-search');
            
        //MOVES SEARCH BUTTON ABOVE SEARCH CONTAINER
            $('#sw-search-button').insertBefore('#sw-mystart-search');
            
        //SEARCH BAR FUNCTIONALITY
        $("div#sw-mystart-search.sw-mystart-nav").unbind();
        $('#sw-search-button').attr('role', 'button');
        $('#sw-search-button').attr('aria-controls', 'sw-mystart-search');
        $('#sw-search-button').attr('aria-expanded', 'false');
        $('#sw-search-button').attr('aria-label', 'search');
        $('#sw-search-button').prop('onclick',null).off('click');
        $("#sw-mystart-search").attr('aria-hidden', 'true');
		 $('#sw-search-button').click(function(e){
        	if ($(window).width() >= 320 ) {
                $('#sw-mystart-search').stop(true, true).animate({width: 'toggle'}, 300);
                $(this).toggleClass('open');

                if( $('#sw-mystart-search').hasClass('showing-search') ){
                    $('#sw-mystart-search').removeClass('showing-search')
                    $('#sw-search-button').attr('aria-expanded', 'false');
                    $('#sw-mystart-search input[type="text"]').attr('tabindex', -1);
                    $("#sw-mystart-search").attr('aria-hidden', 'true');
                }else{
                    $('#sw-mystart-search').addClass('showing-search')
                    $('#sw-search-button').attr('aria-expanded', 'true');
                    $('#sw-mystart-search input[type="text"]').attr('tabindex', 0);
                    $("#sw-mystart-search").attr('aria-hidden', 'false');
                }
            }
        })
        .on('keypress', function(e) { // hooking into the default button trigger so we can stop it and run it only on keyup since we auto focus the search bar
            var code = e.which;
            // 13 = Return, 32 = Space
            if ((code === 13) || (code === 32)) {
                e.preventDefault();
            }
        })
        .on('keyup', function(e) { // Trigger the click event from the keyboard
            var code = e.which;
            // 13 = Return, 32 = Space
            if ((code === 13) || (code === 32)) {
                $(this).click();
            }
        });
        
        //IF TAB IS PRESSED WHILE ON INPUT, GO TO NEXT ELEMENT AND CLOSE INPUT
        $("#sw-search-input").on('keydown', function(e) {
        	var code = e.which;
            // 13 = Tab
            if (code === 9) {
                if(e.shiftKey) {
                       //Focus previous input

                    }
                    else {
                    	e.preventDefault();
                        $('#sw-mystart-search').removeClass('showing-search')
                        $('#sw-search-button').attr('aria-expanded', 'false');
                        $('#sw-search-button').removeClass('open');
                        $('#sw-mystart-search').stop(true, true).animate({width: 'toggle'}, 300);
                        $('#sw-mystart-search input[type="text"]').attr('tabindex', -1);
                        $('li.sw-channel-item:first-of-type > a').focus();
                    }
            }
        }); 

		// ********** HEADER ********** //
		// ADD LOGO
		var logoCheck = "true";
		var logoSrc = jQuery.trim("/cms/lib/MD02215556/Centricity/Template/GlobalAssets/images///logos/aa-logo.png");
		if(logoCheck == "true") {
			var srcSplit = logoSrc.split("/");
			var srcSplitLen = srcSplit.length;
			if((logoSrc != "") && (srcSplit[srcSplitLen - 1] != "default-man.jpg")) {
				$("#gb-logo").append('<a href="/aacps" title="Anne Arundel County Public Schools"><img src="' + logoSrc + '" alt="Anne Arundel County Public Schools" /></a>').removeClass("hidden");
                $("#gb-sitename").addClass("hidden");
			} else {
				$("#gb-logo").append('<a href="/aacps" title="Anne Arundel County Public Schools"><img src="/cms/lib3/SW00000036/Centricity/template/234/logos/fairbanks-logo.png" alt="Anne Arundel County Public Schools" /></a>').removeClass("hidden");
			}
		}
		
		// ADD SITENAME
		var siteNameOne = jQuery.trim('');
		var siteNameTwo = jQuery.trim('');
		if((siteNameOne == "") && (siteNameTwo == "")) {
			var splitLen = 3;
			var siteName = "Anne Arundel County Public Schools";
			siteName = siteName.split(" ");
			var siteNameLength = siteName.length;
			if(siteNameLength > 3){
				siteNameEnd = jQuery.trim(siteName.splice(-splitLen, siteName.length).toString().replace(/,/g, " "));
			} else {
				siteNameEnd = jQuery.trim(siteName.splice(-1, siteName.length).toString().replace(/,/g, " "));
			}
			siteNameBegin = jQuery.trim(siteName.toString().replace(/,/g, " "));
			$("#gb-sitename").prepend("<h1>" + siteNameBegin + "</h1><h2>" + siteNameEnd + "</h2>");
		} else if((siteNameOne != "") && (siteNameTwo == "")) {
			$("#gb-sitename").prepend("<h1>" + siteNameOne + "</h1>");
		} else if((siteNameOne == "") && (siteNameTwo != "")) {
			$("#gb-sitename").prepend("<h2>" + siteNameTwo + "</h2>");
		} else if((siteNameOne != "") && (siteNameTwo != "")) {
			$("#gb-sitename").prepend("<h1>" + siteNameOne + "</h1><h2>" + siteNameTwo + "</h2>");
		}
        
        //SOCIAL MEDIA HOVER
        $(".gb-social-icon").unbind('hover');
        $(".gb-social-icon").hover(function(){
			$(this).addClass("hover");
		}, function(){
			$(this).removeClass("hover");
		});
        
		// ********** CHANNEL BAR ********** //
		$(".sw-channel-item").unbind('hover');
		$(".sw-channel-item").hover(function(){
			$(".sw-channel-item ul").stop(true, true);
			var subList = $(this).children('ul');
			if ($.trim(subList.html()) !== "") {
				subList.slideDown(300, "swing");
			}
			$(this).addClass("hover");
		}, function(){
			$(".sw-channel-dropdown").slideUp(300, "swing");
			$(this).removeClass("hover");
		});
        
        if ($(".sw-channel-dropdown").length < 0) {
        	$(this).remove();
        }
		
		// ********** BODY ********** //
		// AUTO FOCUS SIGN IN FIELD
		$("#swsignin-txt-username").focus();
        
		// CENTER SIGNED OUT MESSAGE AND SIGNIN BUTTON
        if($("div.ui-spn > div > p > span").text() == "You have been signed out."){
            $("div.ui-spn > div").css({"text-align" : "center", "padding" : "50px 0px 50px 0px"});
            //DOC add signed out breadcrumb
            $(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Signed Out</li>");
        }
        
        // ADJUST SIGN IN PAGE
        if($("#swlogin").length){
            $("#sw-content-layout-wrapper .ui-widget.app").addClass("signin");
            $(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Sign In</li>");
        }
        
        // ADJUST REGISTER PAGE
		if($("#swageprompt-txt-birthmonth").length && $("#swageprompt-txt-birthday").length && $("#swageprompt-txt-birthyear").length) {
			$("#sw-content-layout-wrapper .ui-widget.app").addClass("register");
			$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Register</li>");
		}
        
		// ADJUST SEARCH RESULTS PAGE
		if($("#sw-content-layout-wrapper.ui-spn #swsearch-pnl-main").length) {
			$("#sw-content-layout-wrapper").children().wrapAll('<div class="ui-widget app search-results"><div class="ui-widget-detail"></div></div>');
			$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Search Results</li>");
		}
        
        // ADD READ FULL STORY BUTTON TO EACH HEADLINE
        $(".ui-widget.app.headlines").each(function(){
            $(".ui-article", this).each(function(){
                var headlineURL = $("h1 a", this).attr("href");
                $(".ui-article-description", this).append('<a class="custom-read-more" href="' + headlineURL + '">Read Full Story »</a>');
            });
        });
        
        // APPLY RESPONSIVE DIMENSIONS TO CONTENT IMAGES
        $("div.ui-widget.app .ui-widget-detail img")
            .not($("div.ui-widget.app.cs-rs-multimedia-rotator .ui-widget-detail img"))
            .not($("div.ui-widget.app.gallery.json .ui-widget-detail img"))
            .each(function() {
                if ($(this).attr('width') !== undefined && $(this).attr('height') !== undefined) { // IMAGE HAS INLINE DIMENSIONS
                    $(this).css({"display": "inline-block", "width": "100%", "max-width": $(this).attr("width") + "px", "height": "auto", "max-height": $(this).attr("height") + "px"});
                }
        });
        
        // MAKE NAVIGATION APPS ACCESSIBLE
        head.ready("accessibleNavApp", function(){
            $(".ui-widget.app.navigation").csAccessibleNavigationApp();
        });
        	
       $("a.more-link").text("View All");
       $(".headlines .custom-read-more").text("Read More");
       $("<span aria-hidden='true'> >></span>").appendTo(".headlines .custom-read-more");
       $(".headlines .custom-read-more").remove(); //added by Lyndsay - Case 00528886 - 5.1.17
       
       //HIDE SOCIAL MEDIA CONTAINER TOP BORDER IF NO APPS PRESENT
       if (!$("#sw-content-container7 > div").length) {
       		$("#hp-content-bottom-outer").css("border-top", "none");
       }
		
		// ********** FOOTER ********** //
        //REMOVE FAX TEXT IF NO FAX NUMBER IS PRESENT
        if ( $('.fax-number').text().length == 0 ) {
            $('.fax-text').css('display', 'none');
        }
        
        //SCROLL DOWN BUTTON UNDER ROTATOR    
        $(document).on("click keydown", "#back-to-top-btn", function(e) {
        	if(e.type == "click" || e.type == "keydown" && e.keyCode == csKeyCodes.space || e.type == "keydown" && e.keyCode == csKeyCodes.enter) {
            	e.preventDefault();
                
                $("html, body").animate({
                	scrollTop: $("body").offset().top
                }, "slow", function() {
					$("li.sw-channel-item:first-of-type a").focus();
                });
            }
        });

        
		// MOVE SCHOOLWIRES FOOTER STUFF
		$(".gb-schoolwires-footer.logo").html($("#sw-footer-logo").html());
		$(".gb-schoolwires-footer.links").append($("#sw-footer-links li:eq(0) a"));
        
		$(".gb-schoolwires-footer.links").append($("#sw-footer-links li:eq(2) a"));
        
		$(".gb-schoolwires-footer.links").append($("#sw-footer-links li:eq(1) a"));
		$(".gb-schoolwires-footer.links").append($("#sw-footer-copyright"));
        
        //ADD BULLET POINTS BETWEEN SCHOOLWIRES FOOTER LINKS
        $("<span aria-hidden='true'>  •</span>").insertAfter(".gb-schoolwires-footer.links a:nth-child(1), .gb-schoolwires-footer.links a:nth-child(2)");
	}
    
    function rsMenu(sortedSchools) {
        // DEFINE THE HEADER COUNTER
        // NEEDS TO BE DYNAMIC BECAUSE WE CAN'T BE SURE HOW MANY SCHOOL TYPES WE'LL HAVE
        var headerCounter = 0;

        // BUILD THE SCHOOL MENU STRUCTURE
        var schoolHeaders = "";
        var schoolItems = "";
        $.each(sortedSchools, function(type, schools) {
            var typeName = type.replace(/\s/g, "-").replace(/&/g, "and");
            type = type.replace(/charter/, "charter/contract");
            type = type.replace(/other/, "specialty"); //ADDED BY LYNDSAY - CASE 00537026 - 5.22.17

            // SORT SCHOOLS ARRAY ALPHABETICALLY IN CASE SCHOOLS GOT SHIFTED A BIT FROM MULTIPLE TYPE VALUES
            schools.sort(function(a, b) {
                if(a.schoolName > b.schoolName) {
                    return 1;
                }
                if(a.schoolName < b.schoolName) {
                    return -1;
                }
                return 0;
            });

            headerCounter++; // WE HAVE A HEADER SO INCREMENT BY 1
            schoolHeaders += type + ";";

            for(var school in schools) {
                schoolItems += (headerCounter) + ',' + schools[school].schoolName + ',' + schools[school].schoolUrl + ',_self;';
            }
        });

        // BUILD THE GLOBAL ICONS
        var globalIcons = [
            ["Site Utility/8.png", "School Calendar", "/Page/2", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["Student Program/43.png", "School Meals", "http://www.aacps.org/nutrition", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["People/29.png", "Student/Parent Portal", "/Page/1325", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["/cms/lib011/MD02215556/Centricity/template/globalassets/images/icons/global-icon-paper.png", "Vendors and Contractors", "/Page/1391", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["Student Program/8.png", "School Security", "/Page/1039", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["/cms/lib/MD02215556/Centricity/Template/GlobalAssets/icons/57.png", "Safety Hotline", "/Page/8178", "_self"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["Classroom/22.png", "News Releases", "http://www.aacpsschools.org/pressr", "_blank"],  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ["Classroom/4.png", "School Year   2019-2020", "/Page/2504", "_self"]  // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
        ];
        var iconNum = ("8" == "8;7;6;5;4;3;2;1;0") ? 0 : "8";
        var globalLinksHeader = "", globalLinks = "";

        if(iconNum > 0) {
            globalLinksHeader = "Global Links";
            headerCounter++; // WE HAVE A HEADER SO INCREMENT BY 1
        }

        for(var i = 0; i < iconNum; i++) {
            //var linkCheck = (globalIcons[i][2] == "[SITECALENDARLINK]") ? "/Page/2" : globalIcons[i][2];
            globalLinks += (headerCounter) + ',' + globalIcons[i][1] + ',' + globalIcons[i][2] + ',' + globalIcons[i][3] + ';';
        }

        // RESPONSIVE MAIN MENU
        $("body").csRsMenu({
            "breakPoint"            : 768, // SYSTEM BREAK POINTS - 768, 640, 480, 320
            "slideDirection"        : "right-to-left", // OPTIONS - left-to-right, right-to-left   
            "primaryColor"          : "#1A1A1A", // DEFAULT - #1A1A1A
            "secondaryColor"        : "#0C0C0C", // DEFAULT - #0C0C0C
            "accentColor"           : "#BABABA", // DEFAULT - #BABABA
            "menuButtonParent"      : "#gb-mystart-right",
            "showMenuBtnText"       : "yes",
            "menuBtnText"           : "MENU",
            "showSchools"           : "no",
            "showTranslate"         : "no",
            "translateId" : "none",
            "advancedTranlate": {
                "useAdvancedList": "yes", // yes = USE THE NEXT TWO OPTIONS BELOW, no = USE THE REGULAR LIST OF 11 LANGUAGES
            },
            "translateId"           : "none",
            "showAccount"           : "yes",
            "extraMenuOptions"      : schoolHeaders + globalLinksHeader, // EXTRA MENU HEADING NAMES SEPARATED BY A ;
            "extraMenuItems"        : schoolItems + globalLinks, // EXTRA MENU BUTTONS - STRUCTURE IS: MENU-OPTION-NUMBER, ITEM-NAME, ITEM-URL, ITEM-TARGET;
            "siteID"                : "4",
            "allLoaded"             : function(){}
        });
    }
               
    function csAppAccordion() {
    	head.ready("csAppAccordion", function(){
            $(".hp.row.one, .hp.row.two").csAppAccordion({
                "accordionBreakpoints" : [768, 640, 480, 320]
            });
        });
	}
    
    	function tabAccordion(){
		if(!tabAccordionSet) {
			// MAKE HOMEPAGE APPS COLLAPSIBLE FOR SMALL MOBILE DEVICES
			// ACCORDION HEADER CLICK
			$("#sw-content-container2.ui-hp div.ui-widget-header").click(function(){
				if($(this).parent().hasClass("active")) {
					$(this).parent().removeClass("active");
				} else {
					$("#sw-content-container2.ui-hp div.ui-widget.app.active").removeClass("active");
					$(this).parent().addClass("active");
					
					$("html, body").animate({
						scrollTop: $(this).offset().top
					}, 500, function(){
						$("html, body").stop(true, false);
					});
				}
			});
		
			tabAccordionSet = true;
		}
	}
	
	function tabAccordionReset() {
		if(tabAccordionSet) {
			$("#sw-content-container2.ui-hp div.ui-widget-header").unbind("click");
			
			tabAccordionSet = false;
		}
	}
    
    function pageListAccordion() {
    	head.ready("csAppAccordion", function(){
            $(".sp.column.one").csAppAccordion({
                "accordionBreakpoints" : [480, 320]
            });
        });
    }
    
	function useChannelForSection() {
		// USE CHANNEL NAME FOR PAGELIST NAV HEADER IF ONE IS NOT PRESENT
		if(!$("div.sp.column.one .ui-widget-header h1").length) {
			$("div.sp.column.one .ui-widget-header").append("<h1>Students</h1>");
		}
    }
	
	function searchText() {
        //SEARCH TEXT
        $("div#sw-mystart-search input#sw-search-input").removeAttr("placeholder");
        $("div#sw-mystart-search input#sw-search-input").attr("value", "I'm Looking For...");
        
        $("div#sw-mystart-search input#sw-search-input").focus(function() {
        	if($(this).val() == "I'm Looking For...") {
        		$(this).val("");
        	}
        });
        
        $("div#sw-mystart-search input#sw-search-input").blur(function() {
        	if($(this).val() == "") {
        		$(this).val("I'm Looking For...");
        	}
        });
	}
	
	function calendarListView() {
		// AUTO CLICK FUNCTION FOR SCHOOLWIRES CALENDAR LIST VIEW FOR SMALL MOBILE DEVICES
		if(!$(".ui-widget.app.calendar #calendarlist-pnl-specialview.loaded").length){
			$("a.ui-btn-toggle.list").click();
			$("a.ui-btn-toggle.month").click();
			$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("loaded");
		}
		
		switch(csGetBreakPoint()) {
			case "desktop": case "768": case "640":
				$(".ui-widget.app.calendar #calendar").removeClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("ui-helper-hidden");
			break;
			case "480": case "320":
				$(".ui-widget.app.calendar #calendar").addClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").removeClass("ui-helper-hidden");
				$(".ui-btn-toggle.month").removeClass("active");
				$(".ui-btn-toggle.list").addClass("active");
			break;
		}
	}
    
    function modEvents(){
    	head.ready("modEvents", function(){
            if (!$(".upcomingevents .joel-month").length) {
                $(".upcomingevents").modEvents({
                    columns     : "yes",
                    monthLong   : "yes",
                    dayWeek     : "no"
                });
                eventsByDay(".upcomingevents .ui-articles");
                $(".upcomingevents .ui-article-description").each(function() {
                    if(!$(".sw-calendar-block-time", this).length) {
                        $(this).prepend('<span class="sw-calendar-block-time all-day">All Day</span>');
                    }
                    $(".joel-month").append("<span> </span>");
                });
            }
    	});
    }
 
    function eventsByDay(container) {
        var countLi = 0;
        $(".ui-article", container).each(function(){
            if (!$(this).find("h1.ui-article-title.sw-calendar-block-date").size()){
                var moveArticle = $(this).html();
                $(this).parent().prev().children().children().next().append(moveArticle);
                $(this).parent().remove();
            };
        });
    }
	
	function csGetBreakPoint() {
		return window.getComputedStyle(document.querySelector("body"), ":before").getPropertyValue("content").replace(/"|'/g, "");/*"*/
	}

    function buildSchoolMenu() {
    	$.schoolListMinibase({
            'miniBaseDomainID': 4,
            'miniBaseModuleInstanceID': 5443,
            'cache': true,
            'schoolListReady': function(schoolList) {
                var schools = '', sortedSchools = {};
                
                // SORT THE SCHOOL LIST TO TRY TO GET THE EXPECTED TYPES IN THE ORDER WE WANT
                var expectedNameOrder = ["elementary", "middle", "high", "other", "charter/contract"];

                // SORT THE SCHOOLS INTO TYPE OBJECTS
                for(var type in schoolList) {
                	if(schoolList[type].schoolVisible.toLowerCase() == 'true') {
                        var thisType = schoolList[type].schoolType.toLowerCase();

                        // CHECK FOR MULTIPLE COMMA SEPARATED VALUES
                        if(schoolList[type].schoolType.indexOf(",") > -1) {
                            var subTypes = schoolList[type].schoolType.split(",");

                            // LOOP THROUGH THE SUB TYPES
                            for(var subType in subTypes) {
                                var thisSubType = $.trim(subTypes[subType]).toLowerCase();

                                // ADD THIS TYPE OBJECT TO THE SORTED SCHOOLS OBJECT IF IT'S NOT THERE YET
                                if(!sortedSchools.hasOwnProperty(thisSubType)) {
                                    // ADD THIS TYPE VALUE TO THE SORTED SCHOOLS OBJECT AND ADD THIS SCHOOL RECORD
                                    sortedSchools[thisSubType] = [schoolList[type]];
                                } else {
                                    // PUSH THIS SCHOOL RECORD TO THE EXISTING TYPE OBJECT
                                    sortedSchools[thisSubType].push(schoolList[type]);
                                }
                            }
                        }

                        // SINGLE TYPE VALUE
                        else {
                            // ADD THIS TYPE OBJECT TO THE SORTED SCHOOLS OBJECT IF IT'S NOT THERE YET
                            if(!sortedSchools.hasOwnProperty(thisType)) {
                                // ADD THIS TYPE VALUE TO THE SORTED SCHOOLS OBJECT AND ADD THIS SCHOOL RECORD
                                sortedSchools[thisType] = [schoolList[type]];
                            } else {
                                // PUSH THIS SCHOOL RECORD TO THE EXISTING TYPE OBJECT
                                sortedSchools[thisType].push(schoolList[type]);
                            }
                        }
                    }
                }
                
				// BUILD THE SCHOOL MENU STRUCTURE
                var schoolMenuTabs = '<ul id="gb-school-list-tabs" class="ui-clear" role="tablist">';
                var schoolMenuPanels = '<div id="gb-school-list-panels" class="ui-clear">';
               $.each(sortedSchools, function(type, schools) {
                	var typeName = type.replace(/\s/g, "-").replace(/&/g, "and");
                    type = type.replace(/charter/, "charter/contract");
                    type = type.replace(/other/, "specialty"); //ADDED BY LYNDSAY - CASE 00537026 - 5.22.17
                
                    // BUILD TAB
                    schoolMenuTabs += '<li class="gb-school-list-tab ' + typeName + ' border-box" id="gb-' + typeName + '-school-tab" role="tab" aria-selected="false" aria-controls="gb-' + typeName + '-school-panel" tabindex="-1"><span>' + type + '</span></li>';
                    
                    // BUILD PANEL
                    schoolMenuPanels += '<div class="gb-school-list-panel ' + typeName + '" id="gb-' + typeName + '-school-panel" role="tabpanel" aria-hidden="false" aria-labelledby="gb-' + typeName + '-school-tab"><ul class="ui-clear">';
                    for(var school in schools) {
                    	schoolMenuPanels += '<li><a href="' + schools[school].schoolUrl + '">' + schools[school].schoolName + '</a></li>';
                    }
                    schoolMenuPanels += '</ul></div>';
                });
                schoolMenuTabs += '</ul>';
                schoolMenuPanels += '</div>';
                
                // ADD THE COMPLETED MENU TO THE DOM
                $("#gb-school-list").html(schoolMenuTabs + schoolMenuPanels);
                
                // BIND THE TAB PANEL EVENTS
        		SchoolMenuControls();
                
                // BUILD THE RESPONSIVE MENU NOW THAT WE HAVE THE SCHOOLS
                rsMenu(sortedSchools);
            }
        });
        SetCookie('RedirectTo', window.location.href);
    }
    
    function buildSchoolCategoryColumns(schoolArray) {
    	// REFERENCE - http://codepen.io/uxbox/pen/fpLJl
        
        // DEFINE ARRAY THAT WILL HOLD NUMBER OF LINKS FOR EACH COLUMN
        var linksPerCol = [];

        // GET MINIMUM NUMBER OF SCHOOLS PER COLUMN
        var minLinkNum = Math.floor(schoolArray.length / 4);

        // GET THE NUMBER OF UNEVEN REMAINDERS
        var remainders = schoolArray.length - (minLinkNum * 4);

        // LOOP FOUR TIMES BECAUSE THERE ARE FOUR COLUMNS
        for (var i = 0; i < 4; i++) {
            // IF LOOP INDEX IS LESS THAN THE REMAINDER NUMBER
            if (i < remainders) {
                // ADD ONE TO THIS COLUMN LINK NUMBER SO THEN REMAINDER LINKS WILL BE DISTRIBUTED EVENLY
                linksPerCol[i] = minLinkNum + 1;
            } else {
                // THIS WILL BE THE MINIMUM
                linksPerCol[i] = minLinkNum;
            }
        }

        // NOW LOOP AGAIN FOUR TIMES TO BUILD THE COLUMNS - FOUR TIMES BECAUSE THERE ARE FOUR COLUMNS
        var schoolColumns = "";
        for (var i = 0; i < 4; i++) {
            schoolColumns += '<ul>';

            // LOOP THROUGH THE LINKS PER COLUMN ARRAY
            for (var j = 0; j < linksPerCol[i]; j++) {
                // BUILD THE NEXT ITEM INDEX
                var recordIndex = 0;
                for (var k = 0; k < i; k++) {
                    recordIndex += linksPerCol[k];
                }
                
                // BUILD THE LINK ITEM
                schoolColumns += '<li><a href="' + schoolArray[j + recordIndex].siteUrl + '">' + schoolArray[j + recordIndex].siteName + '</a></li>';
            }
            schoolColumns += '</ul>';
        }
		
        // RETURN THE ENTIRE STRUCTURE
        return schoolColumns;
    }
    
    function SchoolMenuControls() {
        // SCHOOLS BUTTON CLICK
        $(".sw-mystart-button.schools a").click(function(e) {
        	e.preventDefault();
            handleSchoolMenuButtonAction();
        });
        
        // SCHOOLS KEYDOWN
        $(".sw-mystart-button.schools a").keydown(function(e) {
        	if(e.keyCode == csKeyCodes.enter || e.keyCode == csKeyCodes.space) {
                e.preventDefault();
                handleSchoolMenuButtonAction();
            }
        });
        
        // SCHOOL TAB FOCUS
        $(".gb-school-list-tab").focus(function() {
        	// ADJUST TAB ATTRIBUTES
            $(".gb-school-list-tab").attr({"tabindex": "-1", "aria-selected": "false"}).removeClass("active");
            $(this).attr({"tabindex": "0", "aria-selected": "true"}).addClass("active");

            // ADJUST PANEL ATTRIBUTES
            $(".gb-school-list-panel").attr({"aria-hidden": "true"}).removeClass("active");
            $("#" + $(this).attr("aria-controls")).attr({"aria-hidden": "false"}).removeClass("active");
        });
        
        //FOCUS SCHOOL TAB WITH HOVER
        $(".gb-school-list-tab span").hover(function() {
        	$(this).parent().focus();
        });	
        
        // IE TAB CLICK
        $(".ie .gb-school-list-tab span").click(function() {
        	$(this).parent().focus();
        });	
        
        // SCHOOL TAB KEYDOWN
        $(".gb-school-list-tab").keydown(function(e) {
        	handleTabListKey($(this), ".gb-school-list-tab", e);
        });
        
        // CLOSE MENU WITH ESC
        $(".gb-school-list-tab, .gb-school-list-panel").keydown(function(e) {
        	if(e.keyCode == csKeyCodes.esc) {
            	e.preventDefault();
                
                $("#gb-school-list-outer").slideUp(function() {
                    // RESET ATTRIBUTES
                    $(".gb-school-list-tab").attr({"tabindex": "-1", "aria-selected": "false"}).removeClass("active");
                    $(".gb-school-list-panel").attr({"aria-hidden": "true"}).removeClass("active");
                }).attr("aria-hidden", "true");

                $(".sw-mystart-button.schools a").attr("aria-expanded", "false").focus();
            }
        });
        
        // FOCUS LEAVES SCHOOL MENU
        $(document).keydown(function() {
        	// GIVE THE DOCUMENT TIME TO FOCUS THE NEXT ITEM
            setTimeout(function() {
            	// IF THIS DROPDOWN OR ANY OF ITS CHILDREN DO NOT HAVE FOCUS
				if(!$("#gb-school-list-outer").find(":focus").length && !$(".sw-mystart-button.schools a").is(":focus")) {
                	// CLOSE THE MENU
                    $("#gb-school-list-outer").slideUp(function() {
                        // RESET ATTRIBUTES
                        $(".gb-school-list-tab").attr({"tabindex": "-1", "aria-selected": "false"}).removeClass("active");
                        $(".gb-school-list-panel").attr({"aria-hidden": "true"}).removeClass("active");
                    }).attr("aria-hidden", "true");

                    $(".sw-mystart-button.schools a").attr("aria-expanded", "false");
                }
            }, 100);
        });
    }
    
    function handleSchoolMenuButtonAction() {
    	// MENU IS NOT OPEN
        if($(".sw-mystart-button.schools a").attr("aria-expanded") == "false") {
            // MAKE SURE ALL ATTRIBUTES ARE RESET
            $(".gb-school-list-tab").attr({"tabindex": "-1", "aria-selected": "false"}).removeClass("active");
            $(".gb-school-list-panel").attr({"aria-hidden": "true"}).removeClass("active");

            // SET ELEMENTARY TAB AND PANEL AS ACTIVE
            $(".gb-school-list-tab:first").attr({"tabindex": "0", "aria-selected": "true"}).addClass("active");
            $(".gb-school-list-panel:first").attr({"aria-hidden": "false"}).addClass("active");
            
            //ADD OPEN CLASS TO SELECT A SCHOOL BUTTON
            $(".sw-mystart-button.schools").addClass("open");

            // OPEN THE MENU
            $("#gb-school-list-outer").slideDown(function() {
                $(".gb-school-list-tab:first").focus();
            }).attr("aria-hidden", "false");

            $(".sw-mystart-button.schools a").attr("aria-expanded", "true");
        }

        // MENU IS OPEN
        else {
            // CLOSE THE MENU
            $("#gb-school-list-outer").slideUp(function() {
                // RESET ATTRIBUTES
                $(".gb-school-list-tab").attr({"tabindex": "-1", "aria-selected": "false"}).removeClass("active");
                $(".gb-school-list-panel").attr({"aria-hidden": "true"}).removeClass("active");
                $(".sw-mystart-button.schools").removeClass("open");
            }).attr("aria-hidden", "true");

            $(".sw-mystart-button.schools a").attr("aria-expanded", "false");
        }
    }
    
    function handleTabListKey(tab, tabClass, event) {
    	var lastIndex = $(tabClass).length - 1;
        
        switch(event.keyCode){
        	// CONSUME LEFT AND UP ARROWS
            case csKeyCodes.left:
            case csKeyCodes.up:
            	event.preventDefault();
                
                // FOCUS THE PREVIOUS TAB
                if (tab.index() == 0) { 
                    $(tabClass + ":last-child").focus();
                } else { 
                    $(tabClass).eq(tab.index() - 1).focus();
                } 
            break;
            
            // CONSUME RIGHT AND DOWN ARROWS
            case csKeyCodes.right:
            case csKeyCodes.down:
            	event.preventDefault();
                
                // FOCUS THE NEXT TAB
                if (tab.index() == lastIndex) { 
                    $(tabClass + ":first-child").focus();
                } else { 
                    $(tabClass).eq(tab.index() + 1).focus();
                }
            break;
            
            // CONSUME HOME KEY
            case csKeyCodes.home:
            	event.preventDefault();
                
                // FOCUS THE FIRST TAB
                $(tabClass + ":first-child").focus();
            break;
            
            // CONSUME END KEY
            case csKeyCodes.end:
            	event.preventDefault();
                
                // FOCUS THE LAST TAB
                $(tabClass + ":last-child").focus();
            break;
        }
    }
    
    function addFramesetTranslate() {
    	//TRANSLATE
        $("#gb-mystart-right").creativeTranslate({
            "type": 2,
            "advancedOptions": {
                "addMethod": "append", // PREPEND OR APPEND THE TRANSLATE ELEMENT
                "dropdownHandleText": '<span>Translate</span><span class="icon-left-arrow"></span>', // ONLY FOR FRAMESET AND API VERSIONS
                "useTranslatedNames": true, // ONLY FOR FRAMESET AND API VERSIONS - true = TRANSLATED LANGUAGE NAME, false = ENGLISH LANGUAGE NAME
                "brandedLayout": 1
            },
            "translateLoaded": function() {} // CALLBACK TO RUN YOUR CUSTOM CODE AFTER EVERYTHING IS READY
        });
    }
    
    function tabbedApps() {
		head.ready("tabbedApps", function() {
			$("#sw-content-container2.ui-hp").tabBlocks();
			$(".tabs-container .clear").remove();
			$(".tabs-container .tab-button").each(function() {
				$(".tab-left", this).remove();
			});
			$("#sw-content-container2.ui-hp").children().not($(".tabs-container")).wrapAll('<div class="tabs-content"></div>');
		});
	}
    
    function modMyStart() {
    	addFramesetTranslate();
        var siteManager =  ('<li>' + $(".sw-mystart-button.manage").clone().html() + '</li>');
        $(siteManager).insertBefore('#sw-myaccount-list li:first-child');
        
