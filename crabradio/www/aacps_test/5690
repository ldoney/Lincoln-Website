        $("#sw-mystart-mypasskey").insertAfter("#sw-mystart-account");
        $("#ui-btn-signin").prependTo(".gb.footer.two");
        $("#ui-btn-signin a span").text("Admin Sign In");
        //$("#ui-btn-register").insertBefore(".gb.footer.two > a");
        $("#ui-btn-register").insertBefore("#ui-btn-signin"); /* added by Matt B - case#3922456 - 08.06.18 */
        $(".sw-accountsettings").attr("title", "Edit your account settings.");
        $(".sw-signout").attr("title", "Sign out of my website.");
    }
