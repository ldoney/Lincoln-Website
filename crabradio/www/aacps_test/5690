<script type="text/javascript">
$(document).ready(function() {
  $("div.ui-article-controls").each(function(){
      if($(this).children("*[id^='hidMediaUrl']").val()==""){
      	$(this).children("a:first").hide();
      }
   });
   
   $('span.ui-podcasts-author').each(function(i) {
      if($.trim($(this).text()) != 'by'){
      	$(this).removeClass('ui-helper-hidden');
      }
  });
  $('span.ui-podcasts-postdate').each(function(i) {
      if($.trim($(this).text()) != 'on'){
      	$(this).removeClass('ui-helper-hidden');
      }
  });		
  $('span.ui-podcasts-length').each(function(i) {
      if($.trim($(this).text()) != 'Length:'){
    	$(this).removeClass('ui-helper-hidden');
      }
  });
 
});
</script>
